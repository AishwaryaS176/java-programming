1.//PROGRAM TO INCREMENT AND RETURN A NUMBER
  public class Main{
    public static void main(String[] args){
        int x=1;
        increment(x);
        System.out.println("num is"+x);
    }

    public static int increment(int x){
        x++;
        System.out.println("num is"+x);
        return x;
        
    
        
    }
}
